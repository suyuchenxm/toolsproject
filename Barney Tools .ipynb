    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "url = \"https://finance.yahoo.com/quote/AAPL/key-statistics?p=AAPL\"\n",
    "def get_us_sector_performance():\n",
    "    output_list =[]\n",
    "    link_list=[]\n",
    "    sec_list=[]\n",
    "    page = requests.get(url)\n",
    "    bs = BeautifulSoup(page.content,'lxml')\n",
    "    sectors = bs.find_all('a',class_='heading1')\n",
    "    for links in sectors:\n",
    "        sec_list.append(links.get_text())\n",
    "        link_list.append('https://eresearch.fidelity.com' + links.get('href'))\n",
    "    \n",
    "    for i in range(len(link_list)):\n",
    "        tuple_ = get_sector_change_and_market_cap(link_list[i])\n",
    "        x,y,z = tuple_\n",
    "        tuple1 = (sec_list[i],x,y,z,link_list[i])\n",
    "        output_list.append(tuple1)\n",
    "    \n",
    "    return output_list\n",
    "\n",
    "get_us_sector_performance()    "
